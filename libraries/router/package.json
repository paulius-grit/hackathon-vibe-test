{
  "name": "@mf-hub/router",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --external react --external react-dom --external @tanstack/react-router",
    "dev": "tsup src/index.ts --format esm --dts --watch --external react --external react-dom --external @tanstack/react-router"
  },
  "peerDependencies": {
    "@tanstack/react-router": "^1.154.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
